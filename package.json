{
  "name": "checkibanapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "pm2Start": "pm2 start ./bin/www --name CheckIban --time -o log/out.log -e log/err.log",
    
    "start:collaudoPowerShell": "powershell -Command \"$env:NODE_ENV='collaudo'; node ./bin/www\"",
    "nodemon:collaudoPowerShell": "powershell -Command \"$env:NODE_ENV='collaudo'; nodemon ./bin/www\""
  },
  "dependencies": {
    "axios": "^1.10.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^17.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mariadb": "^3.4.4",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.14.1",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
